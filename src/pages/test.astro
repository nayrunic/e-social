---
import { SwipableCard } from "@/components/test"
import Layout from "../layouts/Layout.astro"
import { stimuli } from "@/lib/data"

const data = stimuli.map(stimulus => {
    return { src: stimulus.image.src, title: stimulus.title }
})
---

<Layout title="test">
   <SwipableCard images={data} client:load/> 
</Layout>
